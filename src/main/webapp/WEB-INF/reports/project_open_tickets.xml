	<queryString><![CDATA[SELECT t.key_count, t.entered, t.problem, t.scode, t.pri_code, t.assigned_to, t.est_resolution_date, 
first_name, last_name, tp.description AS priority, ts.description AS severity, 
tc.description AS category_name
FROM ticket t 
LEFT JOIN users u ON (t.assigned_to = u.user_id)
LEFT JOIN ticket_priority tp ON (t.pri_code = tp.code)
LEFT JOIN ticket_severity ts ON (t.scode = ts.code)
LEFT JOIN ticket_category tc ON (t.cat_code = tc.id)
WHERE ticketid IN (SELECT ticket_id FROM ticketlink_project WHERE project_id = $P{projectId})
AND closed IS NULL
ORDER BY t.entered]]></queryString>
