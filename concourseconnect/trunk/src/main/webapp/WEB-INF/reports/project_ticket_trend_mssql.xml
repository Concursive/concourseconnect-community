	<queryString><![CDATA[SELECT this_interval, SUM(b) AS backlog_count, SUM(c) AS created_count, SUM(s) AS solved_count FROM (

SELECT (CONVERT(char(10), DATEADD(DAY,-id,CURRENT_TIMESTAMP), 101)) AS this_interval,
0 AS b, 0 AS c, 0 AS s
FROM lookup_number n
WHERE id < $P{number_interval:3}

UNION ALL

SELECT (CONVERT(char(10), DATEADD(DAY,-id,CURRENT_TIMESTAMP), 101)) AS this_interval,
1 AS b, 0 AS c, 0 AS s
FROM lookup_number n, ticket tb
WHERE
((CONVERT(char(10),tb.entered,101) < (CONVERT(char(10), DATEADD(DAY,-id,CURRENT_TIMESTAMP), 101))
OR CONVERT(char(10),tb.entered,101) = (CONVERT(char(10), DATEADD(DAY,-id,CURRENT_TIMESTAMP), 101))) AND
(CONVERT(char(10),tb.closed,101) > (CONVERT(char(10), DATEADD(DAY,-id,CURRENT_TIMESTAMP), 101)) OR tb.closed IS NULL))
AND id < $P{number_interval:3}
AND tb.ticketid IN (SELECT ticket_id FROM ticketlink_project WHERE project_id = $P{projectId})


UNION ALL

SELECT (CONVERT(char(10), DATEADD(DAY,-id,CURRENT_TIMESTAMP), 101)) AS this_interval,
0 AS b, 1 AS c, 0 AS s
FROM lookup_number n, ticket tc
WHERE
((tc.entered::date = (CURRENT_TIMESTAMP  - ( id::text || ' days')::interval)::date))
AND id < $P{number_interval:3}
AND tc.ticketid IN (SELECT ticket_id FROM ticketlink_project WHERE project_id = $P{projectId})

UNION ALL

SELECT (CONVERT(char(10), DATEADD(DAY,-id,CURRENT_TIMESTAMP), 101)) AS this_interval,
0 AS b, 0 AS c, 1 AS s
FROM lookup_number n, ticket ts
WHERE
((ts.closed::date = (CURRENT_TIMESTAMP  - ( id::text || ' days')::interval)::date))
AND id < $P{number_interval:3}
AND ts.ticketid IN (SELECT ticket_id FROM ticketlink_project WHERE project_id = $P{projectId})
) AS some_alias
GROUP BY this_interval
ORDER BY this_interval]]></queryString>
