    <attribute>
      <name>enableJScript</name>
      <required>no</required>
    </attribute>
    <attribute>
      <name>scrollReload</name>
      <required>no</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>rss</name>
      <required>no</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
